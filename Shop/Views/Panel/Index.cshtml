@model IEnumerable<Car>

<div>
    <ul>
        @foreach (var car in Model)
        {
            <li>
                @car.Name;
                -
                <a asp-controller="Panel" asp-action="Edit" class="link-success" asp-route-id="@car.Id">
                    Edit
                </a>
                -
                <a asp-controller="Panel" asp-action="Remove" class="link-success" asp-route-id="@car.Id">
                    Remove
                </a>
            </li>
        }
    </ul>
</div>